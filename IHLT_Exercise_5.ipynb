        "# Padding\n",
        "def pad(batch):\n",
        "    max_len = max(len(x) for x in batch['input_ids'])\n",
        "    padded = [x + [vocab['[PAD]']] * (max_len - len(x)) for x in batch['input_ids']]\n",
        "    return {'input_ids': torch.tensor(padded), 'labels': torch.tensor(batch['label'])}"
